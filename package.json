{
  "name": "sumobits-chat-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-clean": "cd ./android && ./gradlew --stop && cd .. && rm -rf $HOME/.gradle/caches && rm -rf ./android/.gradle/caches/ && rm -rf ./android/app/build",
    "android-debug": "npx react-native run-android --variant=debug --verbose",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier-eslint --write $PWD/src/**/*.js",
    "cleanup": "rm -rf node_modules && rm -f yarn.lock && yarn cache clean",
    "reinstall": "rm -rf node_modules && rm -f yarn.lock && yarn cache clean && yarn install"
  },
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.6.2",
    "@types/node": "^14.6.1",
    "@types/react": "^16.9.43",
    "graphql": "^15.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-native": "^0.63.2",
    "react-native-config": "^1.0.0",
    "react-native-emoji": "^1.8.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.4.0",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1",
    "react-native-vector-icons": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.1.1",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
